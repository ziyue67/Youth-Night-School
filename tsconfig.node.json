{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2020"],
    "module": "esnext",
    "skipLibCheck": true,
 
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true,
 
    "noImplicitAny": false,
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["tailwind.config.ts", "vite-plugin-cloudstudio.js", "postcss.config.js", "eslint.config.js"]
}
